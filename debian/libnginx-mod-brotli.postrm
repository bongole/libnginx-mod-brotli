#!/bin/sh

set -e

case "$1" in
    purge)
        rm -f /etc/nginx/conf.d/brotli.conf
        rm -f /usr/share/nginx/modules/ngx_http_brotli_filter_module.so
        rm -f /usr/share/nginx/modules/ngx_http_brotli_static_module.so
        rm -f /etc/nginx/modules-available/mod-http-brotli.conf
        rm -f /etc/nginx/modules-enabled/50-mod-http-brotli.conf
        ;;
    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
        ;;
    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
esac

#DEBHELPER#

exit 0
